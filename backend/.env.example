# Backend Environment Variables
# Copy this file to .env and fill in your actual values

# Google Generative AI API Key
# Get from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# Climate IQ API Key
# Get from: https://www.climatiq.io/
CLIMATIQ_API_KEY=your_climatiq_api_key_here

# Alternative Climate IQ format (legacy)
CLIMATIQ=your_climatiq_api_key_here

# Orkes API Keys (optional)
# Get from: https://orkes.io/
ORKES_API_KEY_ID=your_orkes_key_id_here
ORKES_API_KEY_SECRET=your_orkes_secret_here
# Optional override for local stubs or regional endpoints
CLIMATIQ_BASE_URL="https://api.climatiq.io/data/v1/estimate"
# Optional legacy alias supported for backwards compatibility
# CLIMATIQ="your-climatiq-api-key"
WEATHER_USER_AGENT="EcoImpactApp/1.0"
RESULT_LOG_PATH=".ecoimpact/results.log"

# Optional: reserved for future Orkes integrations
ORKES_API_KEY_ID=""
ORKES_API_KEY_SECRET=""